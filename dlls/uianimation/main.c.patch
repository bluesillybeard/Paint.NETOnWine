+        if(This->changeHandler != NULL) {
+            FIXME( "animation var change handlers not implemented! (%p), value = %f\n", This, value);
+            return E_NOTIMPL;
+        }
+        if(This->intChangeHandler != NULL) {
+            FIXME( "animation var change handlers not implemented! (%p), value = %f\n", This, value);
+            return E_NOTIMPL;
+        }
+    }
+    return S_OK;
+}
