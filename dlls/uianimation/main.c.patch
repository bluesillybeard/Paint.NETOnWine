+    */
+    struct transition *This = malloc( sizeof(*This) );
+
+    if (!This)
+        return E_OUTOFMEMORY;
+    This->IUIAnimationTransition_iface.lpVtbl = &transition_vtbl;
+    This->ref = 1;
+    This->finalValue = finalValue;
+
+    *obj = &This->IUIAnimationTransition_iface;
+
+    return S_OK;
+}
